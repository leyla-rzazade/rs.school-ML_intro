#https://www.codewars.com/kata/546e2562b03326a88e000020
#Input is integer, output is the integer in which every digit is squared
def square_digits(num):
    res = list(map(int, str(num))) 
    result = list()
    for i in res:
        result.append(i*i)
        
    str1 = ''.join(str(e) for e in result)
    return int(str1)
